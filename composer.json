{
	"name": "guillorrr/talampaya",
	"description": "WordPress development",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		},
		{
			"type": "composer",
			"url": "https://connect.advancedcustomfields.com"
		},
		{
			"type": "package",
			"package": {
				"name": "wpml/sitepress-multilingual-cms",
				"version": "4.6.12",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=6088&user_id={%WORDPRESS_WPML_USER_ID}&subscription_key={%WORDPRESS_WPML_KEY}&version={%VERSION}"
				},
				"require": {
					"ffraenz/private-composer-installer": "^5.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "wpml/wpml-string-translation",
				"version": "3.2.14",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=6092&user_id={%WORDPRESS_WPML_USER_ID}&subscription_key={%WORDPRESS_WPML_KEY}&version={%VERSION}"
				},
				"require": {
					"ffraenz/private-composer-installer": "^5.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "wpml/wp-seo-multilingual",
				"version": "2.1.0",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=3566177&user_id={%WORDPRESS_WPML_USER_ID}&subscription_key={%WORDPRESS_WPML_KEY}&version={%VERSION}"
				},
				"require": {
					"ffraenz/private-composer-installer": "^5.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "wpml/acfml",
				"version": "2.1.3",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=1097589&user_id={%WORDPRESS_WPML_USER_ID}&subscription_key={%WORDPRESS_WPML_KEY}&version={%VERSION}"
				},
				"require": {
					"ffraenz/private-composer-installer": "^5.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "wpml/wpml-import",
				"version": "1.0.1",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=14356615&user_id={%WORDPRESS_WPML_USER_ID}&subscription_key={%WORDPRESS_WPML_KEY}&version={%VERSION}"
				},
				"require": {
					"ffraenz/private-composer-installer": "^5.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "wpml/wpml-all-import",
				"version": "2.3.0",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=720221&user_id={%WORDPRESS_WPML_USER_ID}&subscription_key={%WORDPRESS_WPML_KEY}&version={%VERSION}"
				},
				"require": {
					"ffraenz/private-composer-installer": "^5.0"
				}
			}
		}
	],
	"require": {
		"johnbillion/extended-template-parts": "^2.2.1",
		"timber/timber": "^2.1",
		"twig/html-extra": "^3.0",
		"twig/extra-bundle": "^3.0",
		"vlucas/phpdotenv": "^5.6",
		"illuminate/support": "^10.0"
	},
	"require-dev": {
		"automattic/wordbless": "^0.4.2",
		"yoast/wp-test-utils": "^1.0",
		"wpackagist-plugin/wordpress-importer": "*",
		"wpackagist-plugin/classic-editor": "*",
		"wpackagist-plugin/classic-widgets": "*",
		"wpackagist-plugin/check-email": "*",
		"wpackagist-plugin/moving-media-library": "*",
		"wpackagist-plugin/moving-users": "*",
		"wpackagist-plugin/media-sync": "*",
		"wpackagist-plugin/user-switching": "*",
		"wpackagist-plugin/debug-bar-timber": "*",
		"wpackagist-plugin/debug-bar": "*",
		"wpackagist-plugin/disable-admin-notices": "*",
		"wpackagist-plugin/post-type-switcher": "*",
		"wpackagist-plugin/duplicate-page": "*",
		"wpackagist-plugin/duplicate-post": "*",
		"wpackagist-plugin/user-role-editor": "*",
		"wpackagist-plugin/contact-form-7": "*",
		"wpackagist-plugin/cf7-conditional-fields": "*",
		"wpackagist-plugin/flamingo": "*",
		"wpackagist-plugin/redirection": "*",
		"wpackagist-plugin/akismet": "*",
		"wpackagist-plugin/advanced-custom-fields": "*",
		"wpackagist-plugin/acf-extended": "*",
		"wpackagist-plugin/acf-image-aspect-ratio-crop": "*",
		"wpackagist-plugin/acf-to-custom-database-tables": "*",
		"wpackagist-plugin/tinymce-advanced": "*",
		"wpackagist-plugin/disable-xml-rpc-pingback": "*",
		"wpackagist-plugin/wordpress-seo": "*",
		"wpackagist-plugin/svg-support": "*",
		"wpackagist-plugin/posts-to-posts": "*",
		"wpackagist-plugin/wp-tota11y": "*",
		"wpackagist-plugin/disable-emojis": "*",
		"wpackagist-plugin/wps-hide-login": "*",
		"wpackagist-plugin/login-lockdown": "*",
		"wpackagist-plugin/disable-user-login": "*",
		"wpackagist-plugin/wp-native-php-sessions": "*",
		"wpackagist-plugin/wp-all-import": "*",
		"wpackagist-plugin/migrate-wp-cron-to-action-scheduler": "*",
		"wpackagist-plugin/maintenance": "*",
		"wpackagist-plugin/wp-mail-smtp": "*",
		"wpackagist-plugin/regenerate-thumbnails": "*",
		"wpackagist-theme/poema": "*",
		"wpengine/advanced-custom-fields-pro": "^6.3.2",
		"wpml/sitepress-multilingual-cms": "*",
		"wpml/wpml-string-translation": "*",
		"wpml/wp-seo-multilingual": "*",
		"wpml/acfml": "*",
		"wpml/wpml-import": "*",
		"wpml/wpml-all-import": "*",
		"hellonico/timber-dump-extension": "*"
	},
	"extra": {
		"wordpress-install-dir": "./",
		"installer-paths": {
			"wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			],
			"wp-content/vendor/automattic/wordbless/": [
				"automattic/wordbless"
			]
		},
		"private-composer-installer": {
			"dotenv-path": ".",
			"dotenv-name": ".env"
		}
	},
	"config": {
		"vendor-dir": "wp-content/vendor",
		"allow-plugins": {
			"roots/wordpress-core-installer": false,
			"composer/installers": true,
			"ffraenz/private-composer-installer": true
		}
	},
	"scripts": {
		"test": "phpunit",
		"env": "printenv COMPOSER_AUTH"
	}
}
