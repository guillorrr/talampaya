{
	"name": "talampaya",
	"version": "1.0.0",
	"description": "",
	"keywords": [],
	"author": "@guillorrr <hola@guillo.dev>  (https://guillo.dev/)",
	"contributors": [
		{
			"name": "Guillermo Baron",
			"email": "guillermobaron@gmail.com",
			"url": "https://github.com/guillorrr"
		}
	],
	"license": "ISC",
	"main": "index.js",
	"engineStrict": true,
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	},
	"scripts": {
		"build": "NODE_ENV=production gulp prod",
		"dev": "gulp dev",
		"webpack": "webpack --mode=development",
		"lint:js": "eslint ./src --ext .js,.ts",
		"lint:fix": "eslint ./src --ext .js,.ts --fix",
		"prettier": "npm run prettier:check",
		"prettier:check": "prettier --config .prettierrc './**/*.{md,js,ts,twig,php}' --check",
		"prettier:write": "prettier --config .prettierrc './**/*.{md,js,ts,twig,php}' --write",
		"prepare": "husky",
		"test": "jest",
		"start": "docker compose up",
		"patternlab": "docker compose -f docker-compose.patternlab.yml up",
		"docker:build": "docker compose build --no-cache",
		"docker:delete": "docker compose down -v",
		"docker:rebuild": "docker compose down -v && docker compose build --no-cache",
		"docker:lint:fix": "docker compose run --rm node npm run lint:fix",
		"docker:prettier:check": "docker compose run --rm node npm run prettier:check",
		"docker:prettier:write": "docker compose run --rm node npm run prettier:write",
		"docker:permissions": "docker compose run --rm chmod",
		"docker:composer:update": "docker compose run composer update",
		"docker:composer:dump": "docker compose run composer dump-autoload",
		"dev:create-block": "node ./dev/scripts/create-block.js"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier --config .prettierrc -l --write",
			"eslint ./src --ext .js,.ts --fix"
		],
		"*.(sa|sc|c)ss": "stylelint --fix",
		"*.{html,json,twig,hbs,php}": [
			"prettier --config .prettierrc -l --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.23.9",
		"@babel/core": "^7.24.0",
		"@babel/preset-env": "^7.24.0",
		"@commitlint/cli": "^19.0.3",
		"@commitlint/config-conventional": "^19.0.3",
		"@html-eslint/eslint-plugin": "^0.24.1",
		"@html-eslint/parser": "^0.24.1",
		"@prettier/plugin-php": "^0.22.2",
		"@zackad/prettier-plugin-twig-melody": "^0.6.0",
		"babel-loader": "^9.1.3",
		"browser-sync": "^3.0.2",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"del": "^6.0.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"fancy-log": "^2.0.0",
		"glob": "^10.3.12",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-concat": "^2.6.1",
		"gulp-filter": "^9.0.1",
		"gulp-plumber": "^1.2.1",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.1.4",
		"gulp-sass": "^5.1.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-uglify": "^3.0.2",
		"gulp-vinyl-zip": "^2.5.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.2",
		"node-sass": "^9.0.0",
		"path": "^0.12.7",
		"prettier": "3.2.5",
		"sass": "^1.71.1",
		"stylelint": "^16.6.1",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recess-order": "^5.0.1",
		"stylelint-config-recommended-scss": "^14.0.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.0",
		"stylelint-scss": "^6.3.2",
		"webpack": "^5.90.3",
		"webpack-cli": "^5.1.4",
		"webpack-stream": "^7.0.0"
	}
}
