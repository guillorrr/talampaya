{
	"name": "patternlab/twig",
	"description": "Pattern Lab with Twig renderer",
	"type": "project",
	"require": {
		"php": "^8.1",
		"fakerphp/faker": "^1.24",
		"psr/log": "^1.1 || ^2.0 || ^3.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true
	},
	"scripts": {
		"post-install-cmd": [
			"composer dump-autoload"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	}
}
